<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ac.dars.dao.Function4Dao">
	<select id="getAudioInfo" parameterType = "Function4Dto" resultType = "Function4Dto">
		SELECT
            filename,
			context
		FROM
			function4
		WHERE
			index = #{index}
			;
	</select>
</mapper>